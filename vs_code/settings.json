{
    "editor.tabSize": 2,
    "editor.fontFamily": "Jetbrains Mono, Fira Mono for Powerline, Inconsolata for Powerline, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "files.autoSave": "afterDelay",
    "vsnotes.defaultNotePath": "~/OneDrive - Bluebeam, Inc/notes",
    "vsnotes.defaultNoteTitle": "{title}.{ext}",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Monokai",
    "workbench.iconTheme": "vscode-icons",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[graphql]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
}